alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east

alt + shift - h : yabai -m window --swap west
alt + shift - j : yabai -m window --swap south
alt + shift - k : yabai -m window --swap north
alt + shift - o : yabai -m window --swap east

alt - g : yabai -m window --resize left:-20:0
alt - u : yabai -m window --resize top:0:-20
alt - n : yabai -m window --resize bottom:0:20
alt - m : yabai -m window --resize right:20:0

alt + shift - g : yabai -m window --resize left:20:0
alt + shift - u : yabai -m window --resize top:0:20
alt + shift - n : yabai -m window --resize bottom:0:-20
alt + shift - m : yabai -m window --resize right:-20:0

